<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="model.to">
    <class name="EPGModelTO" table="dbo.T_EPG">
        <id name="ID" type="java.lang.Integer" column="ID"/>
        <property name="BID" type="java.lang.Integer" column="BID"/>
        <property name="TID" type="java.lang.Integer" column="TID"/>
        <property name="CID" type="java.lang.Integer" column="CID"/>
        <property name="EID" type="java.lang.Integer" column="EID"/>
        <property name="EDate" type="java.lang.String" column="EDate"/>
        <property name="ETime" type="java.lang.String" column="ETime"/>
        <property name="EDuration" type="java.lang.String" column="EDuration"/>
        <property name="CName" type="java.lang.String" column="CName"/>
        <property name="getDescription" type="java.lang.String" column="Description"/>
        <property name="title" type="java.lang.String" column="Title"/>
        <property name="userID" type="java.lang.Integer" column="UserID"/>
        <property name="keyWords" type="java.lang.String" column="KeyWords"/>

    </class>
</hibernate-mapping>